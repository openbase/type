<project name="RST" default="dist" basedir=".">

	<description>
		Robotics Systems Types library with common wire schemas
	</description>

	<path id="classpath">
		<pathelement location="@PROTOBUF_JAVA_LIBRARY@" />
	</path>
	<path id="classpath-experimental">
		<path refid="classpath" />
		<pathelement location="@RST_STABLE_PROTO_ROOT@/../../../java/rst-@RST_VERSION@.jar" />
		<pathelement location="@RST_STABLE_PROTO_ROOT@/../../../java/rstsandbox-@RST_VERSION@.jar" />
		<pathelement location="@RST_STABLE_PROTO_ROOT@/../../../java/rstdeprecated-@RST_VERSION@.jar" />
	</path>


	<target name="compile" description="compile the source ">
		<delete dir="classes-experimental" />
		<mkdir dir="classes-experimental" />
		<javac srcdir="@SAFE_CMAKE_CURRENT_BINARY_DIR@/experimental" destdir="classes-experimental" debug="on" deprecation="on" target="1.5" source="1.5">
			<compilerarg value="-Xlint" />
			<classpath refid="classpath-experimental" />
		</javac>
	</target>

	<target name="dist" depends="compile" description="generate the distribution">
		@JAR_LINES@
	</target>

</project>
